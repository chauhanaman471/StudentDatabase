#ifndef COURSE_H_
#define COURSE_H_
#include <map>
#include <string>

class Course {
private:
	std::map<unsigned char,std::string>majorById;
	unsigned int courseKey;
	std::string title;
	unsigned char major;
	float creditPoints;
public:
	Course();
	~Course();
};

#endif /* COURSE_H_ */
